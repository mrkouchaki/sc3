{
        "xapp_name": "sc2",
        "version": "0.0.1",
        "containers": [
            {
                "name": "sc2",
                "image": {
                    "registry": "xApp-registry.local:5008",
                    "name": "sc2",
                    "tag": "0.0.1"
                }
            }
        ],
        "messaging": {
            "ports": [
                {
                    "name": "rmr-data",
                    "container": "sc2",
                    "port": 4560,
                    "txMessages": ["mr_SENT_DATA"],
                    "rxMessages": ["mr_ACK"],
                    "policies": [],
                    "description": "rmr receive data port for sc2"
                },
                {
                    "name": "rmr-route",
                    "container": "sc2",
                    "port": 4561,
                    "description": "rmr route port for sc2"
                }
            ]
        },
        "rmr": {
            "protPort": "tcp:4560",
            "maxSize": 2072,
            "numWorkers": 1,
            "rxMessages": ["mr_ACK"],
            "txMessages": ["mr_SENT_DATA"],
            "policies": []
        }

}
